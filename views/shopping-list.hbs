<div class="container">
<div class="backgroundList">

  <div>
    <h1 class="title">Your shopping list</h1>
    <ul class="list-group shopList mt-5">
      {{#each ingredients}}
      <li class="list-group-item"><i class="fab fa-slack-hash"></i>{{this.quantity}} {{this.measure}} {{this.name}}</li>
      {{#if this.quantity2}}
      <li class="list-group-item"><i class="fab fa-slack-hash"></i>{{this.quantity2}} {{this.measure2}} {{this.name}}
      </li>
      {{/if}}
      {{/each}}
      <ul>
  </div>
  </div>
  </div>